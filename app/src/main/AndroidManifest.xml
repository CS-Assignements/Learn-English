<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cs.learnenglish">

    <application
        android:allowBackup="true"
        android:fullBackupContent="true"
        android:icon="@drawable/learnenglish"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".DialogueActivity"
            android:label="Phrase"
            android:parentActivityName=".Activites.MainActivity"
            android:theme="@style/AppTheme2"/>

        <activity android:name=".Activites.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activites.AlphabetActivity"
            android:label="Alphabet"
            android:parentActivityName=".Activites.MainActivity"
            android:theme="@style/AppTheme2" />
        <activity
            android:name=".Activites.NumbersActivity"
            android:label="Numbers"
            android:parentActivityName=".Activites.MainActivity"
            android:theme="@style/AppTheme2" />
        <activity
            android:name=".Activites.ColorsActivity"
            android:parentActivityName=".Activites.MainActivity"
            android:theme="@style/AppTheme2" />
        <activity
            android:name=".Activites.FamilyMembers"
            android:parentActivityName=".Activites.MainActivity"
            android:theme="@style/AppTheme2" />
        <activity
            android:name=".Activites.AnimalsActivity"
            android:parentActivityName=".Activites.MainActivity"
            android:theme="@style/AppTheme2" />
    </application>

</manifest>